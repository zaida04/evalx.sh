{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "apps/site/.next/**",
        "apps/site/!.next/cache/**",
        "packages/engine/dist/**",
        "apps/runner/dist/**"
      ]
    },
    "lint": {}
  }
}